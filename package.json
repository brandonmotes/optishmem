{
  "main": "lib/binding.js",
  "dependencies": {
    "node-addon-api": "^1.1.0",
    "prebuild-install": "^7.1.2"
  },
  "scripts": {
    "install": "prebuild-install --runtime napi || node-gyp rebuild",
    "rebuild": "node-gyp rebuild",
    "prebuild": "prebuild --runtime napi --all --strip --verbose",
    "upload": "prebuild --runtime napi --upload ${GITHUB_TOKEN}",
    "clean": "node-gyp clean configure build"
  },
  "binary": {
  "napi_versions": [3]
  },
  "gypfile": true,
  "name": "optishmem",
  "version": "1.1.1",
  "description": "A package to access shared memory.",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonmotes/optishmem.git"
  },
  "keywords": [
    "shared-memory"
  ],
  "author": "Brandon Motes",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/optigon/optishmem/issues"
  },
  "homepage": "https://gitlab.com/optigon/optishmem#readme",
  "devDependencies": {
    "prebuild": "^13.0.1"
  }
}
